		$richRESULTS.Select(0, 0); #Unselect all the text
	}
	
	function Update-Text
	{
		#clear the form
		$richRESULTS.SelectAll()
		$richRESULTS.clear()
		
		#lose blanks
		$targets = $lbinput.Lines | Where{ $_ } | ForEach{ $_.Trim() }
		$i = 0
		$stop = $targets.Length
		$stop--
		
		foreach ($line in $targets)
		{
			#format the string
			$targetstring = "('" + $TXTcriteria.text + "' " + $comboQualifier.Text + ' "' + $line + '") ' + $comboAPPEND.Text + "`n"
			#break on last line to omit trailing append
			if ($i -eq $stop)
			{
				#keep the last line, lose the last appended string	
				$targetstring = "('" + $TXTcriteria.text + "' " + $comboQualifier.Text + ' "' + $line + '") '
				break
			}
			$richRESULTS.AppendText($targetstring)
			$i++
		}
		#print the last value
		$richRESULTS.appendtext($targetstring)
		
	}
	
