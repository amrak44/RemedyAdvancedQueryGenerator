	$labelGenerator.Font = [System.Drawing.Font]::new('Cascadia Code', '21.75', [System.Drawing.FontStyle]'Bold')
	$labelGenerator.Location = New-Object System.Drawing.Point(589, 108)
	$labelGenerator.Name = 'labelGenerator'
	$labelGenerator.Size = New-Object System.Drawing.Size(170, 39)
	$labelGenerator.TabIndex = 35
	$labelGenerator.Text = 'Generator'
	$formRemedyAdvancedSearch.ResumeLayout()
	#endregion Generated Form Code

	#----------------------------------------------

	#Save the initial state of the form
	$InitialFormWindowState = $formRemedyAdvancedSearch.WindowState
	#Init the OnLoad event to correct the initial state of the form
	$formRemedyAdvancedSearch.add_Load($Form_StateCorrection_Load)
	#Clean up the control events
	$formRemedyAdvancedSearch.add_FormClosed($Form_Cleanup_FormClosed)
	#Show the Form
	return $formRemedyAdvancedSearch.ShowDialog()

} #End Function

